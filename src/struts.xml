<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
    <package name="a" extends="struts-default" namespace="/">
        <action name="customer-regist" class="action.RegisterAction" method="registerCustomer">
            <result name="no">/regist/accountexit.jsp</result>
            <result name="yes">/customer/customer-center.jsp</result>
        </action>
        <action name="login" class="action.LoginAction" method="login">
            <result name="y">/customer/customer-center.jsp</result>
            <result name="n">/login/loginerror.jsp</result>
        </action>
    </package>
</struts>